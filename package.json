{
  "name": "controle-financeiro",
  "version": "1.0.0",
  "description": "Sistema de Controle Financeiro para PMEs",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build && node scripts/prepare-build.js",
    "build:static": "CI=true NODE_OPTIONS='--max-old-space-size=4096' quasar build",
    "start": "http-server dist/spa -p $PORT -a 0.0.0.0 --proxy http://localhost:$PORT?",
    "preview": "quasar serve dist/spa --port 4173",
    "lint": "eslint --ext .js,.vue ./src",
    "format": "prettier --write \"**/*.{js,vue,css,md,json}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "quasar": "^2.14.2",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.5",
    "chart.js": "^4.4.1",
    "vue-chartjs": "^5.3.0",
    "date-fns": "^3.2.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.6.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz",
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.33"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ]
}